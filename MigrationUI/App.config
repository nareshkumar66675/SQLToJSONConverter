<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.2" />
  </startup>
  <connectionStrings>
    <!--UserMatrix Server Details - Update the connectionString-->
    <add name="UserMatrixDB" connectionString="Data Source=10.2.152.137\sql2012;Integrated Security=False;User ID=sa;Password=abc@1234;Initial Catalog=UM140DB_AMR" />
  </connectionStrings>
  <appSettings>
    <!-- Logging Configurations-->
    <add key="LogFileName" value="MigrateLog" />
    <add key="LogFileTimeStampPattern" value="yyyyMMdd_HHmmss" />
    <!--Log File Path - If Empty, Current Directory-->
    <add key="LogFilePath" value="" />
    <!-- Process Configurations -->
    <add key="BulkCopySize" value="5000" />
    <add key="MaxQueueSize" value="3" />
    <!--Reporting Configurations-->
    <add key="IsReportEnabled" value="True" />
    <add key ="IsReportCheck" value="True"/>
    <!--PreRequisites Configurations-->
    <add key="RunPreRequisites" value="True" />
    <add key ="SqlCommandTimeout" value="500"/>
    <!--Database Check-->
    <add key="AuthDBCheck" value="Site.Site" />
    <add key="LegacyDBCheck" value="GAM.ASSET" />
    <add key="AssetDBCheck" value="DataManagement.Asset" />
    <add key="ReportDBCheck" value="Report.Asset" />
    <!--Cultures Config-->
    <add key="Cultures" value="en-AU;en-US" />
  </appSettings>
</configuration>